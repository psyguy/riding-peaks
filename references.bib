@article{moon_2016_AdvancedCircadianPhase,
  title = {Advanced {{Circadian Phase}} in {{Mania}} and {{Delayed Circadian Phase}} in {{Mixed Mania}} and {{Depression Returned}} to {{Normal}} after {{Treatment}} of {{Bipolar Disorder}}},
  author = {Moon, Joung-Ho and Cho, Chul-Hyun and Son, Gi Hoon and Geum, Dongho and Chung, Sooyoung and Kim, Hyun and Kang, Seung-Gul and Park, Young-Min and Yoon, Ho-Kyoung and Kim, Leen and Jee, Hee-Jung and An, Hyonggin and Kripke, {\relax Daniel}. F. and Lee, Heon-Jeong},
  year = {2016},
