@article{marler_2006_SigmoidallyTransformedCosine,
  title = {The Sigmoidally Transformed Cosine Curve: A Mathematical Model for Circadian Rhythms with Symmetric Non-Sinusoidal Shapes},
  shorttitle = {The Sigmoidally Transformed Cosine Curve},
  author = {Marler, Matthew R. and Gehrman, Philip and Martin, Jennifer L. and {Ancoli-Israel}, Sonia},
